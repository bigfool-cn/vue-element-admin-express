{
  "name": "vue-element-admin-express",
  "script": "node app.js",
  "cwd": "./",
  "watch": [
    "dist",
    "model",
    "router",
    "utils",
    "app.js",
    "pm2.json"
  ],
  "ignore_watch": [
    "node_modules",
    "logs"
  ],
  "watch_options": {
    "followSymlinks": false
  },
  "error_file": "./logs/app-err.log",
  "env": {
    "HOST": "0.0.0.0",
    "PORT": 8082,
    "NODE_ENV": "production"
  }
}
